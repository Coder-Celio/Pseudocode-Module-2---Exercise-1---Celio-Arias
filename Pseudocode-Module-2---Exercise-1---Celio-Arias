Start 
//The goal of the program is to find out the refrigerator module name and capacity in cubic feet
String modelName
Constant Cubic_Inches_In_Cubic_Foot = 1728
num height
num width
num depth
num cubicInches
num cubicFeet

 //input
print "Enter the refrigerator model name: "
input modelName
print "Enter the interior height in inches: "
input height
print "Enter the width in inches: "
input width
print "Enter the dept in inches: "
input depth

//process
cubicInches = height * width * depth
cubicFeet = cubicInches / Cubic_Inches_In_Cubic_Foot

//output
print "Refrigerator model" modelName
print  "Cubic Feet" cubicFeet

End
