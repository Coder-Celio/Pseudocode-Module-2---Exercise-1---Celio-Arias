Start 
//The goal of the program is to find out the refrigerator module name and capacity in cubic feet
String modelName
Constant Cubic_Inches_In_Cubic_Foot = 1728
num height
num width
num depth
num cubicInches
num cubicFeet

 //input
input modelName = "Enter the refrigerator model name: "
print modelName
input height = "Enter the interior height in inches: "
print height
input width = "Enter the width in inches: "
print width
input depth = "Enter the dept in inches: "
print depth

//process
cubicInches = height * width * depth
cubicFeet = cubicInches / Cubic_Inches_In_Cubic_Foot

//output
print "Refrigerator model", modelName, "Cubic Feet", cubicFeet

End
